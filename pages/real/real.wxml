<!--pages/real/real.wxml-->
<view class='headborder'>
  <image src='../../images/logo.png' style='margin-left: 30rpx;height: 80rpx;width: 210rpx;' />
  <view class='headborder-search'>
    <input class='search-value' type='text' placeholder='搜索' placeholder-style='color: white;font-size:30rpx;'/>
    <icon type="search" size="24" color='white' style='margin-top: 16rpx;'/>
  </view>
</view>
  <swiper indicator-dots="true"
  autoplay="true" interval="{{interval}}" duration="{{duration}}" style='height: 360rpx;'>
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" style="width: {{imageWidth}}px;"/>
    </swiper-item>
  </block>
  </swiper>
<view class='title'> 
  <text class='title_value'>行业动态</text>
  <text class='title_value'>政策法规</text>
  <text class='title_value'>研究成果</text>
</view>
<view class='bottom_bk'></view>
<view class='bottom_list'wx:for="{{listDate}}">
  <view class='list_img'>
    <image src='{{item.listImg}}' style="height:150rpx;width:230rpx;"></image>
  </view>
  <view class='list_text'>
    <view class='list_point'>
        <image src='{{item.listPoint}}' style='height:15rpx;width:15rpx;margin-top:14rpx;'></image>
        <text  style='margin-left:30rpx;'>{{item.listValue}}</text>
    </view>
    <view class='list_text'>
      <text style='margin-left:30rpx;font-size:26rpx;'>{{item.listData}}</text>
      <text style='margin-left:30rpx;font-size:26rpx;'>{{item.listDetil}}</text>
    </view>
  </view>
</view>