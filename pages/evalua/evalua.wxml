<view class='top_value'>
  <view class='top_val'>
    <text style='margin-left:90rpx;color:#368BDB;font-size: 46rpx;'>{{community}}</text>
  </view>
  <view class='top_val'>
    <text style='font-size: 36rpx;'>{{mouth}}</text>
    <text style='font-size: 33rpx;'>{{price}}</text>
  </view>
</view>
<view class='mid_value' wx:for="{{listData}}">
