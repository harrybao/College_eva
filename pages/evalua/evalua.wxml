    <text style='margin-left:20rpx;'>{{item.listLeft}}</text>
    <text style='font-size:30rpx;color:#DDDDDD;'>{{item.selectLeft}}</text>
  </view>
  <view class='mid_val'>
    <text style='margin-left:20rpx;'>{{item.listRight}}</text>
    <text style='font-size:30rpx;color:#DDDDDD;'>{{item.selectRight}}</text>
  </view>
</view>
